  paymentPixCode                                    String?
  paymentPixExpirationDate                          DateTime?
  paymentPixQrCode                                  String?
  paymentRefusalReason                              String?
  paymentType                                       String?
  subscriptionStatus                                String?
  subscriptionPlanId                                Int?
