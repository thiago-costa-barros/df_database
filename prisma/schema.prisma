  productUcode                                      String?
  productName                                       String?
  productHasCoProduction                            Boolean?
  affiliateCode                                     String?
  affiliateName                                     String?
  buyerEmail                                        String?
  buyerName                                         String?
  buyerCheckoutPhone                                String?
  buyerZipcode                                      String?
  buyerCountry                                      String?
  buyerNumber                                       String?
  buyerAddress                                      String?
  buyerCity                                         String?
  buyerState                                        String?
  buyerNeighborhood                                 String?
  buyerComplement                                   String?
  buyerCountryIso                                   String?
  producerName                                      String?
  commissionValue1                                  Float?
  commissionCurrencyValue1                          String?
  commissionSource1                                 String?
  purchaseApprovedDate                              DateTime?
  purchaseFullPriceValue                            Float?
  purchaseFullPriceCurrencyValue                    String?
